{% set attrs = '' %}
{% if isPublic is not same as(false) %}
	{% set attrs = attrs ~ ' is-public=true' %}
{% endif %}
{% if username is defined and username is not same as(null) %}
  {% set attrs = attrs ~ ' username=' ~ username|e('html_attr') %}
{% endif %}

<user-roadtrips{{ attrs }}></user-roadtrips>
